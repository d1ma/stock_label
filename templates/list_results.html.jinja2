<!DOCTYPE html>
<html lang="en">
<head>
    <title>Available Spreadsheets</title>
    <script src="/jquery.min.js"></script>
    <script src="/select.js" type="text/javascript"></script>
    <script src="/sorttable.js" type="text/javascript"></script>
    <link rel="stylesheet" type="text/css" href="/style.css"></link>
    <link rel="stylesheet" href="/bootstrap.min.css"></link>
</head>
<body>

<nav id="nav" class="navbar navbar-default navbar-fixed-top">
        <div class="container">
            <div id="navbar" class="navbar-collapse">
              <!--   <div id="header">
                </div> -->
                <ul class="nav navbar-nav">
                    <li><h3>VC Preliminary Results</h3></l3>
                </ul>

                <ul class="nav navbar-nav navbar-right .disabled">
                   
                </ul>
                </div>
            </div>
    </nav>
    <div id="admin_table_container">
        <table class="table sortable paginated" id="IndexTable">
        <thead>
            <th>Name</th>
            <th>Stats</th>
            <th>True labels</th>

            {% for m in r.get_models() %}
                <th>Using {{m}} model</th>
            {% endfor %}
            
            
            
        </thead>
        <tbody>
            {% for tfile, pred in r.get_predictions_per_file() %}
            <tr> 
                <td> {{tfile.basename}} </td>
                <td> {{tfile}} </td>
                <td> {{tfile.tagged_numbers_str()}} </td>
                    {% for m in r.get_models() %}
                <td> {{pred.get(m)}} </td>
                {% endfor %}
            </tr>
            {% endfor %}
        </tbody>

        </table>
    </div>


</body>
</html>